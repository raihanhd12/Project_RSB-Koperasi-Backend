{
    "compilerOptions": {
      "target": "ESNext",
      "module": "nodenext",
      "outDir": "./dist",
      "moduleResolution": "nodenext",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src/**/*.ts", "express.d.ts"],
    "exclude": ["node_modules"]
  }